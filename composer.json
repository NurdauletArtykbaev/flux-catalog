{
  "name": "nurdaulet/flux-catalog",
  "description": "Flux catalog for example package",
  "license": "proprietary",
  "authors": [
    {
      "name": "Nurdaulet",
      "email": "artykbaev.nurdaulet@mail.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nurdaulet\\FluxCatalog\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "spatie/laravel-translatable": "^6.5",
    "staudenmeir/laravel-adjacency-list": "^1.0",
    "filament/filament": "^2.16",
    "filament/spatie-laravel-translatable-plugin": "^2.17"
  },
  "require-dev": {
    "orchestra/testbench": "^8.11"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nurdaulet\\FluxCatalog\\FluxCatalogServiceProvider"
      ],
      "aliases": {
          "TextConverter": "Nurdaulet\\FluxCatalog\\Facades\\TextConverter"
      }
    }
  }
}
